{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>nuxt/renovate-config-nuxt"
  ],
  "packageRules": [
    {
      "groupName": "tailwind",
      "matchPackageNames": [
        "@nuxtjs/tailwindcss",
        "tailwindcss"
      ]
    },
    {
      "matchPackageNames": [
        "typescript"
      ],
      "groupName": "typescript"
    }
  ]
}
